---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Glimpse</title>
 <link href="http://metsci.github.com/glimpse/atom.xml" rel="self"/>
 <link href="http://metsci.github.com/glimpse/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://metsci.github.com/glimpse</id>
 <author>
   <name>Metron Scientific Solutions</name>
   <email>glimpse.metsci.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://metsci.github.com/glimpse{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://metsci.github.com/glimpse{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
