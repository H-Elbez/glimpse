<?xml version="1.0" encoding="ISO-8859-1"?>

<geoskinspec xmlns="http://geo.metsci.com"   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance">

  <feature>
    <featuretype>Coastline</featuretype>
    <shapespec>
      <shapetypespec>Linestring</shapetypespec>
      <painterspec>
        <trackpainterspec>
          <showLines>true</showLines>
          <showPoints>false</showPoints>
          <lineColor>
            <r>0</r> <g>0</g> <b>0</b> <a>.5</a>
          </lineColor>
        </trackpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
    <featuretype>DepthContour</featuretype>
    <shapespec>
      <shapetypespec>Linestring</shapetypespec>
      <painterspec>
        <trackpainterspec>
          <showLines>true</showLines>
          <showPoints>false</showPoints>
          <lineColor>
            <r>.5</r> <g>.5</g> <b>.5</b> <a>.25</a>
          </lineColor>
        </trackpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
    <featuretype>DepthArea</featuretype>
    <shapespec>
      <shapetypespec>Polygon</shapetypespec>
      <painterspec>
        <polygonpainterspec>
          <fill>true</fill>
          <showLines>false</showLines>
          <fillColor>
            <!--
            <r>.7461</r> <g>.8555</g> <b>1.</b> <a>.5</a>
            -->
            <r>190</r> <g>218</g> <b>255.</b> <a>128</a>

          </fillColor>
        </polygonpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
    <featuretype>LandArea</featuretype>
    <shapespec>
      <shapetypespec>Polygon</shapetypespec>
      <painterspec>
        <polygonpainterspec>
          <fill>true</fill>
          <fillColor>
            <r>.7930</r> <g>.8711</g> <b>.5234</b> <a>.5</a>
          </fillColor>
        </polygonpainterspec>
        <annotationpainterspec xs:type="EncAnnotationPainterSpec">
          <color>
            <r>0.</r> <g>0.</g> <b>0.</b> <a>.75</a>
          </color>
          <font>Helvetical_12</font>
          <attributeKey>ObjectName</attributeKey>
        </annotationpainterspec>
      </painterspec>
    </shapespec>
    <shapespec>
      <shapetypespec>Linestring</shapetypespec>
      <painterspec>
        <trackpainterspec>
          <showLines>true</showLines>
          <showPoints>false</showPoints>
          <lineColor>
            <r>.7930</r> <g>.8711</g> <b>.5234</b> <a>.5</a>
          </lineColor>
        </trackpainterspec>
        <annotationpainterspec xs:type="EncAnnotationPainterSpec">
          <color>
            <r>0.</r> <g>0.</g> <b>0.</b> <a>.75</a>
          </color>
          <font>Helvetical_12</font>
          <attributeKey>ObjectName</attributeKey>
        </annotationpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
    <featuretype>LandRegion</featuretype>
    <shapespec>
      <shapetypespec>Polygon</shapetypespec>
      <painterspec>
        <polygonpainterspec>
          <fill>true</fill>
          <fillColor>
            <r>.7930</r> <g>.8711</g> <b>.5234</b> <a>.9</a>
          </fillColor>
          <showLines>false</showLines>
        </polygonpainterspec>
        <annotationpainterspec xs:type="EncAnnotationPainterSpec">
          <color>
            <r>0.</r> <g>0.</g> <b>0.</b> <a>.25</a>
          </color>
         <font>Helvetical_10</font>
          <attributeKey>ObjectName</attributeKey>
        </annotationpainterspec>
      </painterspec>
    </shapespec>
    <shapespec>
      <shapetypespec>Point</shapetypespec>
      <painterspec>
        <trackpainterspec>
          <showLines>true</showLines>
          <pointColor>
            <r>.7930</r> <g>.8711</g> <b>.5234</b> <a>.9</a>
          </pointColor>
        </trackpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
    <featuretype>SeaAreaNamedWaterArea</featuretype>
    <featuretype>Lake</featuretype>
    <shapespec>
      <shapetypespec>Polygon</shapetypespec>
      <painterspec>
        <polygonpainterspec>
          <fill>false</fill>
          <showLines>true</showLines>
          <lineDotted>true</lineDotted>
          <fillColor>
            <r>.7461</r> <g>.8555</g> <b>1.</b> <a>.8</a>
          </fillColor>
          <lineColor>
            <r>.7461</r> <g>.8555</g> <b>1.</b> <a>.8</a>
          </lineColor>
        </polygonpainterspec>
        <annotationpainterspec xs:type="EncAnnotationPainterSpec">
          <color>
            <r>0.</r> <g>0.</g> <b>0.</b> <a>.75</a>
          </color>
          <font>Helvetical_10</font>
          <attributeKey>ObjectName</attributeKey>
        </annotationpainterspec>
      </painterspec>
    </shapespec>
  </feature>

  <feature>
